    "@tensorflow/tfjs-backend-wasm": "^4.2.0",
    "@tensorflow/tfjs-backend-webgl": "^4.2.0",
    "@tensorflow/tfjs-vis": "^1.5.1",
    "assert": "^2.0.0",
    "d3-selection": "^3.0.0",
    "fs-extra": "^11.1.0",
    "jasmine": "^4.5.0",
    "jimp": "^0.17.9",
    "lodash": "^4.17.21",
    "node-gyp": "^9.3.1",
    "util": "^0.12.5"
  }
}
